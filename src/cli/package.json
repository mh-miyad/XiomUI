{
  "name": "xiom-ui",
  "version": "0.1.0",
  "description": "A CLI to add beautiful UI components to your React project",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "xiom-ui": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --watch",
    "start": "node dist/index.js",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "ui",
    "components",
    "react",
    "tailwindcss",
    "cli",
    "xiom",
    "shadcn"
  ],
  "author": "Marc <your@email.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/YOUR_USERNAME/xiom-ui.git"
  },
  "homepage": "https://xiom-ui.vercel.app",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "ora": "^7.0.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.0",
    "@types/prompts": "^2.4.9",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0"
  }
}
